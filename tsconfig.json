{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "dist",
    "rootDir": "src",
    "module": "CommonJS",
    "moduleResolution": "node16"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/global.d.ts",
    "src/nativewind-env.d.ts",
    "nativewind-env.d.ts"
  ]
}